<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!-- For menu caption in Action Bar -->
  <UIActionBar>
    <tooltip>
      <OfficeOnlineOpen>Edit in Office Online</OfficeOnlineOpen>
    </tooltip>
  </UIActionBar>
  <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
  <UITabForm>
    <label>
      <officeOnlineOpen>Edit in Office Online</officeOnlineOpen>
    </label>
  </UITabForm>

  <!-- For client UI -->
  <OfficeonlineEditorClient>
    <EditButtonTitle>Edit in Office Online</EditButtonTitle>
    <ViewButtonTitle>View in Office Online</ViewButtonTitle>
    <UpdateBannerTitle>The document has been updated.</UpdateBannerTitle>
    <ReloadButtonTitle>Reload</ReloadButtonTitle>
    <EditorTitle>Online Editor</EditorTitle>
    <ErrorTitle>Error</ErrorTitle>
    <InfoTitle>Information</InfoTitle>
  </OfficeonlineEditorClient>
  
      
  <!-- For new document form -->
  <UINewDocumentForm>
    <label>
      <option>
        <MicrosoftBlankWordOpenXMLDocument>Microsoft Word Document (*.docx) *</MicrosoftBlankWordOpenXMLDocument>
        <MicrosoftBlankExcelOpenXMLDocument>Microsoft Excel Workbook (*.xlsx) *</MicrosoftBlankExcelOpenXMLDocument>
        <MicrosoftBlankPowerPointOpenXMLDocument>Microsoft PowerPoint Presentation *</MicrosoftBlankPowerPointOpenXMLDocument>
        <MicrosoftTemplateWordOpenXMLDocument>Microsoft Template Document (*.docx) *</MicrosoftTemplateWordOpenXMLDocument>
        <MicrosoftTemplateExcelOpenXMLDocument>Microsoft Template Workbook (*.xlsx) *</MicrosoftTemplateExcelOpenXMLDocument>
        <MicrosoftTemplatePowerPointOpenXMLDocument>Microsoft Template Presentation *</MicrosoftTemplatePowerPointOpenXMLDocument>
      </option>
    </label>
  </UINewDocumentForm>

  <!-- Messages -->
  <OfficeonlineEditor>
    <message>
      <ViewerNotFound>Online document editor not found</ViewerNotFound>
    </message>
    <error>
      <EditorCannotBeCreated>Editor cannot be created. Please retry.</EditorCannotBeCreated>
      <FileNotFound>File not found. Please retry.</FileNotFound>
      <WrongExtension>Wrong file extension. Please retry.</WrongExtension>
      <ActionNotFound>Action not found. Please retry.</ActionNotFound>
      <CannotReadDocument>Cannot read the document. Please retry.</CannotReadDocument>
      <DocumentIdRequired>Wrong request: document ID required.</DocumentIdRequired>
    </error>
  </OfficeonlineEditor>
</bundle>